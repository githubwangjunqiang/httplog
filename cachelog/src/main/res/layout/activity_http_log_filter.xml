<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HttpLogFilterActivity">

    <TextView
        android:id="@+id/xp_log_http_activity_tvtit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#B92D6FF4"
        android:padding="10dp"
        android:text="哈喽"
        android:textColor="#ffffff"
        android:textSize="14dp"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/xp_log_http_activity_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/xp_log_http_activity_tvtit" />

</androidx.constraintlayout.widget.ConstraintLayout>